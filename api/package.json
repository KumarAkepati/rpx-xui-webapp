{
    "name": "jui",
    "version": "0.0.0",
    "license": "MIT",
    "scripts": {
        "ng": "ng",
        "build": "NODE_ENV=production webpack --config ./webpack-node.config.js",
        "builddev": "NODE_ENV=development webpack --config ./webpack-node.config.js",
        "coverage": "npx nyc mocha",
        "lint": "npx tslint --project .",
        "watch": "nodemon --watch '**/*.ts' --exec ts-node local.ts",
        "watchserver": "nodemon --watch '**/*.ts' --exec ts-node server.ts",
        "test": "mocha || true",
        "test:watch": "mocha --watch --recursive --watch-extensions ts",
        "start": "cd ../dist/jui-backend && node server.js"
    },
    "private": true,
    "dependencies": {}
}
